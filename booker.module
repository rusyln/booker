<?php

/**
 * Implements hook_form_alter().
 */


  function booker_form_alter(&$form, &$form_state, $form_id) {
    if ($form_id == 'node_booking_form') {
      $form['#validate'][] = 'booker_form_validate';
      dpm($form['#validate']);
    }
  } 

  
  function booker_form_validate($form, &$form_state) {
    dpm($form_state);
  }
  